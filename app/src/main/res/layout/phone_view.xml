<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
  <ImageView
      android:layout_width="wrap_content"
      android:layout_height="wrap_content"

      android:id="@+id/phone_iv"

      />
    <TextView
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"

       android:id="@+id/phone_pn"
        android:layout_marginLeft="29dp"
        android:layout_marginStart="29dp"
        android:layout_marginTop="10dp"
        android:textSize="20sp"
        android:layout_alignParentTop="true"
        android:layout_toRightOf="@+id/phone_iv"
        android:layout_toEndOf="@+id/phone_iv" />
      <TextView
          android:layout_width="wrap_content"
          android:layout_height="wrap_content"

          android:id="@+id/phone_vn"


          android:layout_alignBottom="@+id/phone_iv"
          android:layout_alignLeft="@+id/phone_pn"
          android:layout_alignStart="@+id/phone_pn" />







</RelativeLayout>
